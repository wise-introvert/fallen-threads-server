{
  "name": "express-typescript-starter",
  "version": "1.1.6",
  "author": "rjmacarthy",
  "private": false,
  "scripts": {
    "dev": "nodemon .",
    "build": "tsc",
    "start": "node dist/index.js",
    "format": "prettier --write 'src/**/*ts'",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:test": "tslint -c tslint.json 'spec/**/*.ts'",
    "format:test": "prettier --write 'spec/**/*ts'"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "~1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^11.10.1",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "strip-ansi": "^7.1.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.0",
    "concurrently": "^8.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.1.6"
  }
}
